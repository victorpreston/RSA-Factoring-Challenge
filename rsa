#!/usr/bin/python3
from sys import argv
import math
"""Get the first two factors for 'num' value"""


def value_prime(value):
    """Checking prime property of a number"""
    j = 3
    if value % 2 == 0:
        return False
    while j * j <= value:
        if value % j == 0:
            return False
        j += 2
    return True


def factorize(value):
    """Get & print factors of a value (num)"""
    if value % 2 == 0:
        j = 2
        print("{}={}*{}".format(value, int(value/j), j))
    else:
        sroot = math.sqrt(value)
        if sroot % 1 == 0:
            print("{}={}*{}".format(value, sroot, int(value/sroot)))
            return
        sroot = int(sroot) + 1
        for j in range(3, sroot, +2):
            if value % j == 0:
                if value_prime(j):
                    print("{}={}*{}".format(value, int(value/j), j))
                    return


def factorization(filename):
    """Reading the factor file"""
    with open(filename, encoding="utf-8") as my_file:
        for j in my_file.readlines():
            num = int(j)
            result = factorize(num)


if __name__ == "__main__":
    factors(argv[1])
